#include<stdio.h>

int main()
{
    char name[10];
    int age;

    printf("Name: ");
    scanf("%s", &name);
    printf("Age: ");
    scanf("%d", &age);
 
    if(age>=18)
        printf("You are eligible for voting.\n");
    else if(age<18) 
        printf("You are not eligible for voting.\n");  
    else
        printf("Invalid Age, Please Try Again.\n");

    return 0;
}
